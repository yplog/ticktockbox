# TickTockBox Configuration
# Copy this file to .env and modify the values as needed

# Server Configuration
PORT=3000

# QuestDB Configuration
# PostgreSQL wire protocol URL for QuestDB
QUESTDB_URL=localhost:8812
QUESTDB_USER=admin
QUESTDB_PASSWORD=quest
QUESTDB_NAME=qdb
# SSL mode: disable, require, verify-ca, verify-full
QUESTDB_SSL_MODE=disable
# HTTP API URL for QuestDB (for partition management)
QUESTDB_HTTP_URL=http://localhost:9000

# RabbitMQ Configuration
# AMQP connection URL for RabbitMQ
RABBITMQ_URL=amqp://guest:guest@localhost:5672/

# Optional Configuration
# LOG_LEVEL=info
# SCHEDULER_INTERVAL=10s 